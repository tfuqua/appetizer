{
  "name": "appetizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:front-end\" \"npm run start:node\"",
    "start:front-end": "node scripts/start.js",
    "start:node": "cross-env NODE_ENV=development nodemon index.js --exec babel-node --presets es2015,stage-2",
    "build": "node scripts/build.js && npm run build:node",
    "build:node": "cross-env NODE_ENV=production babel server -d build/server --presets es2015,stage-2",
    "node:prod": "cross-env NODE_ENV=production nodemon build/server/index.js",
    "deploy": "cross-env NODE_ENV=production pm2 start build/server/index.js --name app",
    "stop": "pm2 stop app"
  },
  "proxy": "http://localhost:8000",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "http": "^0.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jss": "^9.3.3",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-beta.17",
    "mongoose": "^4.13.5",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-jss": "^8.1.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^4.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
